package I_Q_Java;
import java.util.TreeSet;
public class Apractice {
public static void main(String[] args) {
		
	

 int [] data = {2, 34, 324, 234, 234, 24,45,667676,4,46,0};
 
 	int largest = data[0];
 	int smallest = data[0];
 	
 	
 	for (int i = 0; i<data.length; i++) {
 		if (data[i]>largest) {
 			largest = data[i];
 		}else if (data[i]<smallest) {
 			smallest = data[i];
 			
 			
 		}
 	}
 		System.out.println("largest Values is \t " +largest);
 		System.out.println("smallest is \t " +smallest);
 
 
 
 
 
 
}


}