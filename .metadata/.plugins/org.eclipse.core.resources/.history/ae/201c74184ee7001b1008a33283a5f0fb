package Day7;
public class ConstructorOverloading {
		int x,y;
		double z;	

ConstructorOverloading(){ //1-defualt cons
		x=100;
		y=200;
	System.out.println(x+y);
	}	
ConstructorOverloading(int a, int b){ //2-parameterized cons
		x=a;
		y=b;
	System.out.println(x+y);
}
ConstructorOverloading(int a, double b ){ //3-parameterized cons
		x=a;
		z=b;
	System.out.println(z+b);	
}
ConstructorOverloading(int a, int b, double c){ //4-parameterized cons
		x=a;
		y=b;
		z=c;
	System.out.println(x+y+z);		
}
public static void main(String[] args) {
	ConstructorOverloading co=new ConstructorOverloading();
	ConstructorOverloading con=new ConstructorOverloading(10,15);
	ConstructorOverloading cons=new ConstructorOverloading(10,15.99);
	ConstructorOverloading cont=new ConstructorOverloading(50,25.99);
	}

}
